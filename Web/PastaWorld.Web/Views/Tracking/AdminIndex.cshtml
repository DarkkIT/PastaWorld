@model PastaWorld.Web.ViewModels.Orders.OrdersListViewModel;
@{
    this.ViewData["Title"] = "Admin Tracking";
}

<head>

</head>

<div class="text-hide mt-5">
    <p>MoveDown</p>
</div>

<h1 class="text-center">Not Finalized Orders </h1>

<div>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Image</th>
                <th scope="col">Id</th>
                <th scope="col">Clien</th>
                <th scope="col">Address</th>
                <th scope="col">Status</th>
            </tr>
        </thead>

        @foreach (var order in Model.Orders)
        {
            <tbody>

                <tr>
                    <td><img src="~/images/profils/ProfilOne.jpg" style="max-height: 60px;" alt="image" /></td>
                    <td>@order.Id</td>
                    <td>@order.CliendId</td>
                    <td>@order.Address</td>
                    <td>@order.Status</td>
                </tr>

            </tbody>
        }
    </table>

</div>